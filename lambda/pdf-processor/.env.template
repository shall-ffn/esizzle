# Environment Configuration Template for Local Lambda Debugging
# Copy this file to .env and update the values for your local environment

# S3 Configuration
S3_BUCKET_NAME=ffncorp.com-dev-db-cluster

# Database Configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_db_password
DB_NAME=loanmaster
DB_PORT=3306
DB_CHARSET=utf8mb4

# API Configuration (for callbacks)
API_BASE_URL=http://localhost:5000
API_SERVICE_TOKEN=your_service_token
API_TIMEOUT=30
API_RETRY_ATTEMPTS=3

# AWS Configuration (if using real AWS services)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# AWS_REGION=us-east-1

# Debug Configuration
DEBUG_MODE=true
LOG_LEVEL=DEBUG
